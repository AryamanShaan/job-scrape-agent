# Database connection string
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/job_scrape

# LLM provider: "ollama", "gemini", "claude", or "openai"
LLM_PROVIDER=ollama

# Ollama settings (only needed if LLM_PROVIDER=ollama)
OLLAMA_MODEL=llama3
OLLAMA_BASE_URL=http://localhost:11434

# API key (only needed if LLM_PROVIDER is gemini, claude, or openai)
LLM_API_KEY=
